<?xml version="1.0"?>
<its:rules xmlns:its="http://www.w3.org/2005/11/its" version="1.0">
  <!-- General Rules -->
  <its:preserveSpaceRule selector="//*" space="preserve"/>
  <its:translateRule selector="/*" translate="no"/>
  <its:escapeRule selector="//*" escape="no"/>
  <its:escapeRule selector="//*/@*" escape="no"/>

  <!-- plugin -->
  <its:translateRule selector="/plugin/@name" translate="yes"/>
  <its:translateRule selector="/plugin/author" translate="yes"/>
  <its:translateRule selector="/plugin/description" translate="yes"/>
</its:rules>
